{
  "mcpServers": {
    "mcp-tasks": {
      "command": "npx",
      "args": ["-y", "mcp-tasks"]
    },
    "scm": {
      "command": "/home/babbitt/.local/bin/scm",
      "args": ["mcp"]
    },
    "qdrant": {
      "command": "uvx",
      "args": ["mcp-server-qdrant"],
      "env": {
        "QDRANT_LOCAL_PATH": ".vectors/qdrant",
        "COLLECTION_NAME": "angzarr-codebase",
        "EMBEDDING_MODEL": "sentence-transformers/all-MiniLM-L6-v2"
      }
    }
  }
}
